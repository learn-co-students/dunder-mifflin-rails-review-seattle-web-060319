<h1>New Employee</h1>

<%=form_for @employee do |f| %>
<%= f.label :first_name %>
<%= f.text_field :first_name %>
<%= f.label :last_name %>
<%= f.text_field :last_name %>
<%= f.label :title %>
<%= f.text_field :title %>
<%= f.label :alias %>
<%= f.text_field :alias %>
<%= f.label :office %>
<%= f.text_field :office %>
<%= f.collection_select(:dog_id, @dogs, :id, :name) %>
<%= f.submit %>
<%end%>
